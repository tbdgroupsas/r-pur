<?xml version="1.0" encoding="utf-8"?>
<odoo>
<record id="stock_quant_tree_sbo" model="ir.ui.view">
  <field name="name">stock.quant.sbo.tree</field>
  <field name="model">stock.quant</field>
  <field name="inherit_id" ref="stock.view_stock_quant_tree_editable"/>
  <field name="arch" type="xml">
    <xpath expr="//field[@name='available_quantity']" position="after">
      <field name="lst_price" avg="Averrage Sell Price"/>
      <field name="value_price" sum="Sum of Value Price"/>
      <field name="unit_cost" avg="Averrage cost"/>
    </xpath>
  </field>
</record>

</odoo>